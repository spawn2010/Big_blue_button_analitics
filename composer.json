{
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=8.0",
    "slim/slim": "4.*",
    "slim/psr7": "dev-master",
    "nyholm/psr7": "dev-master",
    "nyholm/psr7-server": "dev-master",
    "guzzlehttp/psr7": "dev-master",
    "http-interop/http-factory-guzzle": "dev-master",
    "laminas/laminas-diactoros": "2.8.x-dev",
    "php-di/php-di": "v7.x-dev",
    "monolog/monolog": "2.3.5",
    "robmorgan/phinx": "0.12.10",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/",
      "Config\\": "config/",
      "Templates\\": "templates/"
    }
  }
}